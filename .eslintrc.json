{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "quotes": "warn",
        "semi": ["error", "always"],
        "comma-dangle": ["error", "always-multiline"],
        "no-unused-vars": "warn",
        "react/jsx-key": "warn",
        "react/prop-types": "warn"
    }
}
